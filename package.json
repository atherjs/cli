{
  "name": "@atherjs/cli",
  "version": "0.1.0",
  "type": "module",
  "description": "Cli tool to create a Ather project",
  "keywords": [
    "Atherjs",
    "Atherjs CLI"
  ],
  "author": "Aniket Khote",
  "license": "MIT",
  "bin": {
    "ather": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts",
    "release": "bumpp --commit --push --tag && npm publish --access public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "bumpp": "^9.4.2",
    "bun-types": "^1.1.21",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@atherjs/ather": "latest",
    "@clack/prompts": "^0.7.0",
    "@inquirer/prompts": "^5.3.8",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "figlet": "^1.7.0"
  }
}
